             x:Class="Test1MAUI.Views.Pages.FlyoutSample.FlyoutSamplePage"
             xmlns:Views="clr-namespace:Test1MAUI.Views.Pages.FlyoutSample"
             xmlns:local="clr-namespace:Test1MAUI"
             Title="FlyoutSamplePage">
